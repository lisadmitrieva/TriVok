        "# Методы API VK\n",
        "\n",
        "vk_get_posts_url = 'https://api.vk.com/method/wall.get'\n",
        "vk_get_comments_url = 'https://api.vk.com/method/wall.getComments'"
      ],
      "metadata": {
        "id": "hAfHIO-PQFyr"
      },
      "execution_count": 17,
      "outputs": []
