        "# Найдём комментарии к рассматриваемым постам\n",
        "\n",
        "comments = []\n",
        "\n",
        "for _, post in posts.iterrows():\n",
        "    response = requests.get(\n",
        "        vk_get_comments_url,\n",
        "        params=vk_params | {\n",
        "            \"count\": 100,\n",
        "            \"offset\": 0,\n",
        "            \"owner_id\": post.owner_id,\n",
        "            \"post_id\": post.id,\n",
        "        },\n",
        "    ).json()[\"response\"][\"items\"]\n",
        "\n",
        "    comments.extend([\n",
        "        c | {\"post_id\": post.id}\n",
        "        for c in response\n",
        "    ])\n",
        "\n",
        "comments = pd.json_normalize(comments, sep=\"_\")\n",
        "\n",
        "comments.date = comments.date.apply(datetime.fromtimestamp)\n",
        "\n",
        "comments.info()"
