    {
      "cell_type": "code",
      "source": [
        "# Скачаем посты со страницы\n",
        "\n",
        "posts = []\n",
        "date_start = datetime(2023, 6, 12)\n",
        "date_end = datetime(2024, 6, 12)\n",
        "group = \"trivokzaladepo\"\n",
        "\n",
        "offset = 0\n",
        "\n",
        "while True:\n",
        "    response = requests.get(\n",
        "        vk_get_posts_url,\n",
        "        params=vk_params | {\n",
        "            \"count\": 100,\n",
        "            \"offset\": offset,\n",
        "            \"owner_id\": group,\n",
        "        },\n",
        "    ).json()[\"response\"][\"items\"]\n",
        "\n",
        "    offset += 100\n",
        "\n",
        "    if not response:\n",
        "        break\n",
        "\n",
        "    posts.extend(response)\n",
        "\n",
        "    last_date = datetime.fromtimestamp(response[-1][\"date\"])\n",
        "    if last_date < date_start:\n",
        "        break\n",
        "\n",
        "del offset\n",
        "\n",
        "posts = pd.json_normalize(posts, sep=\"_\")\n",
        "\n",
        "posts.date = posts.date.apply(datetime.fromtimestamp)\n",
        "\n",
        "posts = posts[(date_start <= posts.date) & (posts.date <= date_end)]\n",
        "\n",
        "posts.info()"
