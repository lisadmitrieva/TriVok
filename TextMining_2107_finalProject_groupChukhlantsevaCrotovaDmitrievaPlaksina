    {
      "cell_type": "code",
      "source": [
        "# Переведём тональность в численный вид\n",
        "\n",
        "def get_sentiment_number(row):\n",
        "    label, score = row\n",
        "    sign = 0\n",
        "    if label == \"POSITIVE\":\n",
        "        sign = 1\n",
        "    elif label == \"NEGATIVE\":\n",
        "        sign = -1\n",
        "\n",
        "    return sign * score\n",
        "\n",
        "comments[\"sentiment_number\"] = comments[[\"sentiment_label\", \"sentiment_score\"]].apply(get_sentiment_number, axis=1)"
      ],
      "metadata": {
        "id": "zdZelAAhHnYH"
      },
      "execution_count": 31,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Найдём среднюю тональность комментариев под каждым постом\n",
        "\n",
        "mean_comment_sentiments = comments.groupby(\"post_id\").sentiment_number.mean()\n",
        "\n",
        "posts[\"comments_sentiment\"] = posts.join(mean_comment_sentiments, on=\"id\").sentiment_number"
      ],
      "metadata": {
        "id": "ZYr4aNETIfCE"
      },
